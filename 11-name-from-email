public String nameFromEmail(String email) {
    //code here
    list<String> listEmail = email.splitByCharacterTypeCamelCase();

    String s1 = listEmail.get(0);
    String s2 = listEmail.get(2);

    String firstName = s1.substring(0, 1).toUpperCase() + s1.substring(1).toLowerCase();
    String lastName = s2.substring(0, 1).toUpperCase() + s2.substring(1).toLowerCase();

    return firstName + ' ' + lastName; 
}